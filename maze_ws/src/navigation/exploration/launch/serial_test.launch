<launch>

  <node name="get_walls" pkg="nav_main" type="walls.py" respawn="true">
  <param name="debug" value="true"/>
  </node>

<!-- Launch node to test serial communication implementation -->
  <node name="serial_node" pkg="exploration" type="serial_driver.py" respawn="true" respawn_delay="2" output="screen">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="57600"/> 
        <param name="timeout" value="0.5"/>
  </node>
</launch>
